.ui_uploader_cell {
  width: 670rpx;
  box-sizing: border-box;
  position:relative;
  left:40rpx;
  top:100rpx;
}
.ui_uploader_item{
  float: left;
  position: relative;
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 20rpx;
  margin-right:23rpx;
}
.ui_uploader {
  float: left;
  position: relative;
  width: 200rpx;
  height: 200rpx;
  border: 2rpx solid #d9d9d9;
  box-sizing: border-box;
  background: rgba(0,0,0,0.02);
  border: 1px solid rgba(0,0,0,0.15);
  border-radius:4rpx;
  margin-bottom: 20rpx;
  margin-right:23rpx;
}

.ui_uploader:before {
  content: " ";
  position: absolute;
  width: 4rpx;
  height: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader:after {
  content: " ";
  position: absolute;
  height: 4rpx;
  width: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}
.ui_uploader_item_icon{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}
.ui_uploader_item image {
  width: 100%;
  height: 100%;
}