/*checkbox 整体大小  */
checkbox {
  width: 15rpx;
  height: 15rpx;
  padding:10rpx;
}
/*checkbox 选项框大小  */
checkbox .wx-checkbox-input {
  width: 15rpx;
  height: 15rpx;
  padding:10rpx;
  border-radius: 50%;
}

/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 38rpx;
  height: 38rpx;
  line-height: 38rpx;
  text-align: center;
  border-radius: 50%;
  font-size: 22rpx;
  background: #1677FF;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

