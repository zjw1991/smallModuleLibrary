
.uploadIdcard_wrap{
  text-align: center;
}
.uploadIdcard_wrap .comIdcard{
  display: inline-block;
  position: relative;
  width: 320rpx;
  height: 164rpx;
  border-radius:4px;
  vertical-align: middle;
}
.uploadIdcard_wrap .guohui{
  margin-left:30rpx;
}
.uploadIdcard_wrap .comIdcard .idcardImage{
  position: absolute;
  left:0;
  top:0;
  width: 100%;
  height:100%;
  border-radius:4px;
}

.uploadIdcard_wrap .comIdcard .ui_uploader {
  position: relative;
  width: 320rpx;
  height: 164rpx;
  background: #FBFBFB;
  text-align: center;
  border: 1px dashed rgba(217,217,217,1);
  border-radius:4rpx;
}
.uploadIdcard_wrap .comIdcard .uploadiconWrap{
  margin-top:40rpx;
}
.uploadIdcard_wrap .comIdcard .uploadimgText{
  color:#999;
  font-size:20rpx;
  margin-top:10rpx;
}
.uploadIdcard_wrap .comIdcard .addText{
  font-size:28rpx;
}
.uploadIdcard_wrap .comIdcard .uploadicon{
  width:36rpx;
  margin-right:8rpx;
}
.uploadIdcardTitle{
  font-size:32rpx;
  color:#999;
  margin-left:32rpx;
  margin-bottom:24rpx;
}

.ui_uploader_item_icon{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}
.ui_uploader_item image {
  width: 100%;
  height: 100%;
  border-radius:4px;
}